<header>
  <%= image_tag("logo.jpg", :alt => "SIBC", :class => "round") %>
  <nav class="round">
    <ul>
      <li><%= link_to "Home", root_path %></li>
      <li><%= link_to "Enquiry Service", enquiry_service_path %></li>
      <li><%= link_to "Services and Support", services_support_path %></li>
      <li><%= link_to "Recording Tools", recording_tools_path %></li>
      <li><%= link_to "Submit your Sightings", submit_your_sightings_path %></li>
      <li><%= link_to "About Us", about_path %></li>
      <li><%= link_to "Contact Us", contact_path %></li>
      <% if signed_in? %>
      <li><%= link_to "Sign out", signout_path, :method => :delete %></li>
      <% else %>
      <li><%= link_to "Sign in", signin_path %></li>
      <% end %>
    </ul>
  </nav>
</header>
