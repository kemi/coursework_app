<!DOCTYPE html>
<html>
  <head>
    <title><%= title %></title>
    <%= csrf_meta_tag %>
    <%= render 'layouts/stylesheets' %>
    
  </head>
  <body>
    <div class="container">
      <%= render 'layouts/header' %>
      <section class="round">
       <table width = '720'> 
         <tr>   
         <td width = '500'><%= yield %></td>
         <td style="vertical-align:top"><%= render 'layouts/sidebar' %></td>
         </tr>
       </table>
      </section>
      <%= render 'layouts/footer' %>
      <%= debug(params) if Rails.env.development? %>
      </section>
    </div>
 
  </body>
</html>
